{
  "name": "critiqo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"node server/index.js\" \"npm run client\"",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "server": "cd server && node index.js",
    "deploy": "cd client && npm run build && gh-pages -d build"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "gh-pages": "^6.3.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.56.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "node-fetch": "^2.7.0",
    "openid": "^2.0.13",
    "react-scripts": "^5.0.1",
    "xml2js": "^0.6.2"
  }
}